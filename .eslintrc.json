{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "consistent-return": "off",
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "quotes": [2, "double", { "avoidEscape": true }]
  }
}
