{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "implicit-arrow-linebreak": "warn",
    "consistent-return": "off",
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "quotes": [2, "double", { "avoidEscape": true }],
    "camelcase": "off",
    "operator-linebreak": ["off", "none"],
    "function-paren-newline": "off",
    "object-curly-newline": "off"
  }
}
